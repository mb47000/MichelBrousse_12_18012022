{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "source": {
        "include": [
            "src"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
        "plugins/markdown",
        "better-docs/component",
        "better-docs/category"
    ],
    "templates": {
        "better-docs": {
            "name": "SportSee Documentation",
            "component": {
                "wrapper": "docComponents/wrapper-component.js"
            }
        }
    },
    "opts": {
        "destination": "docs",
        "encoding": "utf8",
        "recurse": true,
        "verbose": true,
        "readme": "README.md",
        "template": "node_modules/better-docs/"
    }
}